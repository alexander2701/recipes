<table class="table" id="resultsTable">
  <thead>
    <tr>
      <th scope="col"></th>
      <th scope="col">Naam</th>
      <th scope="col">Bereidingstijd</th>
      <th scope="col">Soort recept</th>
      <th scope="col">Boek</th>
    </tr>
  </thead>
  <tbody>
    <% searchResult.forEach(recipe => { %>
    <tr>
      <td><i class="
        <% if (recipe.favorite == true){ %>
        fas fa-heart icon-red
        <% }else{ %>
        far fa-heart
        <% } %>
        "></i></td>
      <td><a href="/recipes/<%= recipe.name %>"><%= recipe.name %></a></td>
      <td><%= recipe.preparationTime %> minuten</td>
      <td><%= recipe.type %></td>
      <td><%= recipe.book %></td>
    </tr>
    <% }) %>
  </tbody>
</table>
